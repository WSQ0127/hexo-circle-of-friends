{
  "version": 2,
  "public": true,
  "builds": [

    { "src": "path/to/server.js", "use": "vercel-node@18.X" }

  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/main.py"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "*"
        }
      ]
    }
  ]
}
